{
  "name": "siv",
  "version": "1.1.0",
  "main": "app.js",
  "engines": {
    "node": "5.1.1"
  },
  "scripts": {
    "make-logfile": "touch ./logs/siv.log",
    "link-config": "ln -s -r -f ./package.json ./app/package.json",
    "download-icons": "wget --input-file=icons.txt --directory-prefix=./app/icons",
    "setup": "mkdir -p ./extensions ./logs ./app/icons",
    "postsetup": "npm run download-icons && npm run make-logfile && npm run link-config",
    "test": "tape test/**/*.js | tap-spec",
    "posttest": "standard test/**/*.js app/**/*.js"
  },
  "devDependencies": {
    "electron-prebuilt": "0.36.1",
    "mock-fs": "3.5.0",
    "react-addons-perf": "0.14.3",
    "standard": "5.4.1",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "aws-sdk": "2.2.18",
    "cli-table2": "0.1.9",
    "javascript-natural-sort": "0.7.1",
    "minimist": "1.2.0",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "redux": "3.0.5",
    "request": "2.65.0",
    "victor": "1.1.0",
    "winston": "2.1.1"
  }
}

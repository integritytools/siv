{
  "name": "siv",
  "version": "1.2.1",
  "main": "app.js",
  "engines": {
    "node": "5.1.1"
  },
  "scripts": {
    "start": "electron app",
    "webpack": "webpack",
    "link-config": "ln -s -r -f ./package.json ./app/package.json",
    "download-icons": "wget --input-file=icons.txt --directory-prefix=./app/icons",
    "setup": "mkdir -p ./app/icons",
    "postsetup": "npm run download-icons && npm run link-config",
    "test": "tape 'test/**/*.js' | tap-spec",
    "tape": "tape",
    "posttest": "standard test/**/*.js app/**/*.js"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.11.1",
    "devtron": "^1.2.1",
    "electron-prebuilt": "1.2.7",
    "enzyme": "^2.4.1",
    "mock-fs": "^3.5.0",
    "react-addons-perf": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "sinon": "^1.17.4",
    "standard": "5.4.1",
    "tap-spec": "4.1.1",
    "tape": "4.4.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "cli-table2": "0.1.9",
    "javascript-natural-sort": "0.7.1",
    "mathjs": "3.1.4",
    "minimist": "1.2.0",
    "move-element": "0.1.0",
    "react": "15.0.1",
    "react-addons-update": "15.0.1",
    "react-dom": "15.0.1",
    "react-hyperscript": "2.4.0",
    "redux": "3.3.1",
    "siv-components": "0.0.0",
    "victor": "1.1.0"
  }
}

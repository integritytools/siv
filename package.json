{
  "name": "siv",
  "version": "1.2.1",
  "main": "app.js",
  "engines": {
    "node": "5.1.1"
  },
  "scripts": {
    "start": "electron app",
    "link-config": "ln -s -r -f ./package.json ./app/package.json",
    "download-icons": "wget --input-file=icons.txt --directory-prefix=./app/icons",
    "setup": "mkdir -p ./extensions ./logs ./app/icons",
    "postsetup": "npm run download-icons && npm run link-config",
    "test": "tape 'test/**/*.js' | tap-spec",
    "tape": "tape",
    "posttest": "standard test/**/*.js app/**/*.js"
  },
  "devDependencies": {
    "devtron": "^1.2.1",
    "electron-prebuilt": "1.2.3",
    "mock-fs": "3.5.0",
    "react-addons-perf": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "standard": "5.4.1",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "cli-table2": "0.1.9",
    "javascript-natural-sort": "0.7.1",
    "mathjs": "3.1.4",
    "minimist": "1.2.0",
    "move-element": "0.1.0",
    "react": "15.0.1",
    "react-addons-update": "15.0.1",
    "react-dom": "15.0.1",
    "react-hyperscript": "2.4.0",
    "redux": "3.3.1",
    "siv-components": "0.0.0",
    "victor": "1.1.0"
  },
  "extensions": [
    {
      "name": "Image",
      "id": "WdMG",
      "path": "siv-image"
    },
    {
      "name": "Caliper",
      "id": "GyMG",
      "path": "siv-caliper"
    },
    {
      "name": "SCCIR",
      "id": "G9Wd",
      "path": "siv-sccir"
    },
    {
      "name": "Example",
      "id": "kd8F",
      "path": "siv-example"
    }
  ]
}

{
  "name": "siv",
  "version": "1.2.1",
  "main": "app.js",
  "engines": {
    "node": "5.1.1"
  },
  "scripts": {
    "link-config": "ln -s -r -f ./package.json ./app/package.json",
    "download-icons": "wget --input-file=icons.txt --directory-prefix=./app/icons",
    "setup": "mkdir -p ./app/icons",
    "postsetup": "npm run download-icons && npm run link-config",
    "start": "electron app",
    "webpack": "webpack",
    "lint": "eslint ./app/**/*.js --ignore-path .gitignore || true",
    "test": "jest"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.11.1",
    "devtron": "^1.2.1",
    "electron-prebuilt": "1.2.7",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.0",
    "jest-cli": "^14.1.0",
    "mock-fs": "^3.5.0",
    "react-addons-perf": "15.0.1",
    "react-addons-test-utils": "15.0.1",
    "react-test-renderer": "^15.3.0",
    "sinon": "^1.17.4",
    "standard": "5.4.1",
    "tap-spec": "4.1.1",
    "tape": "4.4.0",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "cli-table2": "0.1.9",
    "javascript-natural-sort": "0.7.1",
    "mathjs": "3.1.4",
    "minimist": "1.2.0",
    "move-element": "0.1.0",
    "react": "15.3.0",
    "react-addons-update": "15.3.0",
    "react-dom": "15.3.0",
    "react-hyperscript": "2.4.0",
    "redux": "3.3.1",
    "siv-components": "0.0.0",
    "victor": "1.1.0"
  },
  "jest": {
    "automock": false,
    "rootDir": "tests",
    "testRegex": ".+\\.js$"
  }
}

{
  "name": "siv",
  "version": "1.0.0",
  "main": "app.js",
  "engines": {
    "node": "5.1.1"
  },
  "scripts": {
    "link:html": "ln -s -r -f ./src/siv.html ./build/app/siv.html",
    "link:css": "ln -s -r -f ./src/siv.css ./build/app/siv.css",
    "link:config": "ln -s -r -f ./package.json ./build/app/package.json",
    "make:links": "npm run link:html && npm run link:css && npm run link:config",
    "make:logfile": "touch build/logs/siv.log",
    "download:icons": "wget --input-file=icons.txt --directory-prefix=./build/app/icons",
    "setup": "mkdir -p build build/extensions build/logs build/app/icons",
    "postsetup": "npm run download:icons && npm run make:links && npm run make:logfile",
    "babel": "babel src/ --watch --out-dir build/app/ --presets react,es2015",
    "mocha": "mocha",
    "eslint": "eslint"
  },
  "devDependencies": {
    "babel-cli": "6.0.14",
    "babel-eslint": "4.1.5",
    "babel-preset-es2015": "6.0.14",
    "babel-preset-react": "6.0.14",
    "chai": "3.4.1",
    "electron-prebuilt": "0.36.1",
    "eslint": "1.9.0",
    "eslint-config-airbnb": "1.0.0",
    "eslint-plugin-react": "3.8.0",
    "mocha": "2.3.3",
    "mock-fs": "3.5.0",
    "react-addons-perf": "0.14.3"
  },
  "dependencies": {
    "aws-sdk": "2.2.18",
    "cli-table2": "0.1.9",
    "minimist": "1.2.0",
    "react": "0.14.3",
    "react-dom": "0.14.3",
    "redux": "3.0.5",
    "request": "2.65.0",
    "victor": "1.1.0",
    "winston": "2.1.1"
  }
}
